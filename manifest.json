{
    "name": "Pinger",
    "version": "1.0.0",
    "description": "Tracking updates to websites",
    "manifest_version": 3,
    "permissions": ["alarms", "notifications", "storage", "tabs"],
    "author": "Dewey Su, Joseph Oronto-Pratt, Henry Xie",
    "background": {
        "service_worker": "script.js"
    },
    "action":{
        "default_popup": "index.html",
        "default_title": "Pinger"
    }
}
